<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.GameOver">
   <children>
      <ImageView fx:id="GameOver" fitHeight="400.0" fitWidth="659.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="yes" mnemonicParsing="false" onAction="#chooseLevels" prefHeight="27.0" prefWidth="59.0" text="Yes!" textFill="#1f2208" AnchorPane.bottomAnchor="120.0" AnchorPane.leftAnchor="150.0">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font></Button>
      <Button fx:id="exit" alignment="BOTTOM_RIGHT" layoutX="400.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="30.0" prefWidth="50.0" text="Exit" AnchorPane.bottomAnchor="120.0" AnchorPane.leftAnchor="400.0" AnchorPane.rightAnchor="209.0">
         <font>
            <Font name="Arial Rounded MT Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="190.0" layoutY="4.0" prefHeight="73.0" prefWidth="244.0" text="Do you want to Continue?" textFill="#e4ce25">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
